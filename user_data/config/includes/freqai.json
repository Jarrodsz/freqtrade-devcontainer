{
  "freqai": {
    "enabled": true,
    "purge_old_models": 2,
    "train_period_days": 3,
    "identifier": "uniqe-id",
    "feature_parameters": {
      "include_timeframes": [
        "15m"
      ],
      "include_corr_pairlist": [
        "BTC/USDT",
        "ETH/USDT"
      ],
      "label_period_candles": 20,
      "include_shifted_candles": 2,
      "DI_threshold": 0.9,
      "weight_factor": 0.9,
      "principal_component_analysis": false,
      "use_SVM_to_remove_outliers": true,
      "indicator_periods_candles": [
        10,
        20
      ]
    },
    "data_split_parameters": {
      "test_size": 0,
      "random_state": 1
    },
    "model_training_parameters": {
      "n_estimators": 800
    }
  }
}
